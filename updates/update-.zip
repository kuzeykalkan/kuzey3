    window.location.href="https://codecanyon.net/item/twigo-exclusive-mobile-theme-belloo-dating-software/37469822";  
  }  
  if(action == 'helpCenter'){
    $crisp.push(["do", "message:send", ["text","Hi! I'd like to get help."]]);
  }
  if(action == 'crypto'){
    $crisp.push(["do", "message:send", ["text","Hi! I need assistence with crypto payment."]]);
  }  
  if(action == 'sales'){
    $crisp.push(["do", "message:send", ["text","Hi! I'd like to know more about pricing."]]);    
  }
  if(action == 'extendedUpgrade'){
    $crisp.push(["do", "message:send", ["text","Hi! I'd like to know more about how to upgrade my regular license to extended with discount."]]);    
  } 
  if(action == 'affiliates'){
